
@{
    ViewBag.Title = "TooLongFile";
    Layout = "~/Areas/Default/Views/Shared/_Layout.cshtml";
}
@section main{
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="error-template">
                    <h1>
                        Oops!
                    </h1>
                    <h2>
                        404.13 Too long file
                    </h2>
                    <div class="error-details">
                        Sorry, you can not upload files with length greater than 30MB!
                    </div>
                    <div class="error-actions">
                        <a href="@Url.Action("Index", "Home")" class="btn btn-primary btn-lg">
                            <span class="glyphicon glyphicon-home"></span>
                            Take Me Home
                        </a><a href="mailto:gamanovichpavel@gmail.com" class="btn btn-default btn-lg"><span class="glyphicon glyphicon-envelope"></span> Contact Support </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
}




